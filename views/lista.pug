extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-teal
        h3 Listagem de teses
      .w3-container
        table.w3-table-all
          tr
            th Titulo
            th Data
            th Tipo
            th Informação alunos
          each tese in lista
            tr
              td=tese.titulo
              td=tese.data
              td=tese.tipo
              each aluno in tese.alunos
                td=aluno.nome
                td=aluno.identificacao
                td=aluno.curso
                td=aluno.instituicao
